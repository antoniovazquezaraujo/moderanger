ModeRanger {
  Block = BlockContent (space+ Block)*
  BlockContent = NoteGroup | Note
  Note = Duration? space* SimpleNote
  SimpleNote = SilenceSign | NoteValue
  NoteGroup = Duration space* "(" space* Block space* ")"
  Duration = DurationValue DurationSign
  DurationValue = digit+ "n" "."? | digit+ "m" | digit+ "t"
  NoteValue = "-"? digit+
  DurationSign = ":"
  SilenceSign = "s"
  space = " "
} 