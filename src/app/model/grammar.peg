BLOCK:= 
    head = BLOCK_CONTENT
    tail = {NOTE_SEPARATOR content = BLOCK}*
BLOCK_CONTENT:= valur= NOTE | value= NOTE_GROUP
NOTE:= SIMPLE_NOTE | TIMED_NOTE
SIMPLE_NOTE:= NOTE_VALUE | SILENCE_SIGN
TIMED_NOTE := duration= DURATION SIMPLE_NOTE
NOTE_GROUP:= duration= DURATION GROUP_START block=BLOCK GROUP_END
GROUP_START:= '\('
GROUP_END:= '\)'
DURATION:= value = '[0-9]+' DURATION_SIGN
NOTE_VALUE:= value='-?\d+'
DURATION_SIGN:= 'd'
SILENCE_SIGN:= 's'
NOTE_SEPARATOR:= ' '
 
